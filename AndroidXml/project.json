      "dependencies": {
    	"NETStandard.Library": "1.5.0-rc2-24027",
        "System.Runtime.Serialization.Primitives": "4.1.1-rc2-24027",
        "System.Runtime.Serialization.Formatters": "4.0.0-rc3-24111",
        "CoreCompat.System.Drawing": "1.0.0-beta003"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Drawing": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    }
  },
  "runtimes": {
    "win7-x64": { }
  }
}
